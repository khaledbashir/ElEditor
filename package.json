{
  "name": "spreadsheet-template",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=11.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:fast": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "dev:port": "next dev -p 3001",
    "dev:port:chat": "next dev -p 3001",
    "dev:chat": "cross-env NODE_OPTIONS='--max-old-space-size=4096' node scripts/dev-with-chat.js",
    "dev:simple": "next dev",
    "dev:open": "open http://localhost:3000/chat || xdg-open http://localhost:3000/chat || start http://localhost:3000/chat; next dev",
    "dev:open:port": "open http://localhost:3001/chat || xdg-open http://localhost:3001/chat || start http://localhost:3001/chat; next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "start:port": "next start -p 3001",
    "lint": "eslint .",
    "check-types": "tsc",
    "init": "npx tambo init",
    "clean": "rm -rf .next node_modules/.cache",
    "clean:full": "rm -rf .next node_modules package-lock.json pnpm-lock.yaml",
    "dev:optimized": "cross-env NODE_OPTIONS='--max-old-space-size=4096' next dev --turbo",
    "fix:codegpt-port": "node scripts/fix-codegpt-port.js",
    "fix:codegpt-port:force": "node scripts/fix-codegpt-port.js --force"
  },
  "dependencies": {
    "@blocksuite/blocks": "^0.10.0",
    "@blocksuite/editor": "^0.10.0",
    "@blocksuite/store": "^0.10.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-tooltip": "1.2.8",
    "@silevis/reactgrid": "^4.1.17",
    "@tailwindcss/oxide": "^4.1.16",
    "@tambo-ai/react": "^0.58.1",
    "@tambo-ai/typescript-sdk": "^0.75.1",
    "class-variance-authority": "^0.7.1",
    "dompurify": "^3.3.0",
    "framer-motion": "^12.23.24",
    "highlight.js": "^11.11.1",
    "json-stringify-pretty-compact": "^4.0.0",
    "lucide-react": "^0.546.0",
    "next": "15.5.2",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "recharts": "^3.3.0",
    "streamdown": "^1.4.0",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.27",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "postcss": "^8.5.6",
    "react-devtools": "^7.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
